    [Nonterminal('additiveexpression', [Nonterminal('number', [Symbol('NUMBERLITERAL', '1')]), Symbol('__2_+', '+'), 
        Nonterminal('additiveexpression', [Nonterminal('multiplicativeexpression', [Nonterminal('number', [Symbol('NUMBERLITERAL', '2')]), Symbol('__0_*', '*'), Nonterminal('number', [Symbol('NUMBERLITERAL', '2
')])]), Symbol('__3_-', '-'), Nonterminal('additiveexpression', [Nonterminal('number', [Symbol('NUMBERLITERAL', '2')]), Symbol('__2_+', '+'), Nonterminal('number', [Symbol('NUMBERLITE
RAL', '1000')])])])])])])

//right now
ADD(
    NUM(1), 
    SYMBOL(+), 
    ADD(
        MUL(
            NUM(2), 
            SYMBOL(*), 
            NUM(2)),
        SYMBOL(-)
        ADD(
            NUM(2), 
            SYMBOL(+), 
            NUM(1000)
            )
        )
    )
